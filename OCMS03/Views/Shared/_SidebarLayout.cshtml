    <script type="text/javascript" charset="UTF-8" src="https://maps.googleapis.com/maps-api-v3/api/js/41/6/intl/en_gb/util.js"></script>

    <title>@ViewData["Title"] - OCMS03</title>
    <style>
        .input-validation-error {
            border-color: red;
            background-color: #fee;
        }
    </style>
    @*<link href="~/tables/datatables-bs4/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
        <link href="~/tables/datatables-responsive/css/responsive.bootstrap4.min.css" rel="stylesheet" />*@
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" />

    <script src="~/css/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/css/lib/jquery-validation/dist/jquery.validate.min.js"></script>
